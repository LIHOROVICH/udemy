function app_function(a){if(!a){a={}}return new Vue({el:!store_global.state.GLOBAL.iFrameId?"#headF":"#iframe_bind_vue",store:store_global,data:function data(){return{store:store_global.state,menu_items_hovered:[]}},beforeCreate:function beforeCreate(){if(!this.$store){this.$store=this.$options.store}this.$store.dispatch("setCurrSection",this.$store.getters.getSection());if(!a.auth){document.dispatchEvent(new CustomEvent("init.store"));this.$store.commit("set_window_width",{width:window.innerWidth})}},mounted:function mounted(){window.addEventListener("resize",this.resizeHandler)},beforeDestroy:function beforeDestroy(){window.removeEventListener("resize",this.resizeHandler)},methods:{resizeHandler:function resizeHandler(){var a=this;setTimeout(function(){a.$store.commit("set_window_width",{width:window.innerWidth})},100)},setMenuItemHovered:function setMenuItemHovered(a){if(!this.isMenuItemHovered(a)){this.menu_items_hovered.push(a)}},isMenuItemHovered:function isMenuItemHovered(a){return this.menu_items_hovered.indexOf(a)!==-1}}})}var app=app_function();