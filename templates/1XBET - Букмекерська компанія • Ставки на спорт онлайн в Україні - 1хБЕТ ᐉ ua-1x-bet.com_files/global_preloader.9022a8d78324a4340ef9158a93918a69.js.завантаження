window.addEventListener("load",hideGlobalPreloader);function showGlobalPreloader(){document.querySelector(".main-preloader").style.display="flex"}function hideGlobalPreloader(){document.querySelector(".main-preloader").style.display="none";window.global_preloader_hided=true;var a;if(typeof Event==="function"){a=new Event("hide_global_preloader")}else{a=document.createEvent("Event");a.initEvent("hide_global_preloader",true,true)}window.dispatchEvent(a)}